   "source": [
    "class Solution:\n",
    "    def reverseWords(self, s: str) -> str:\n",
    "        listt = s.split()\n",
    "        strr = str()\n",
    "        for i in listt:\n",
    "            strr += (i[::-1] + ' ')\n",
    "        return strr[:-1]"
   ]
  },
