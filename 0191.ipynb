   "source": [
    "class Solution:\n",
    "    def hammingWeight(self, n: int) -> int:\n",
    "        c = 0\n",
    "        while n != 0:\n",
    "            if n & 1 == 1:\n",
    "                c += 1\n",
    "            n >>= 1\n",
    "        return c"
   ]
